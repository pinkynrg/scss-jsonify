{
  "name": "scss-jsonify",
  "version": "1.0.4",
  "description": "Converts CSS/SCSS to JSON and back.",
  "main": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "cssjson": "dist/cli.js"
  },
  "scripts": {
    "build": "npm run lint && npm run test && tsc -p ./",
    "lint": "npx eslint '**/*.{ts,js}' --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "build:toc": "npx doctoc ./README.md",
    "start": "ts-node-script src/cli.ts"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/through2": "^2.0.41",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.15.0",
    "eslint-plugin-deprecate": "^0.7.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "ts-node": "^10.9.2",
    "typescript": "^4.1.3"
  },
  "license": "MIT",
  "author": {
    "name": "Aram Kocharyan https://github.com/aramk"
  },
  "bugs": {
    "url": "https://github.com/aramk/CSSJSON/issues"
  },
  "homepage": "https://github.com/aramk/CSSJSON",
  "keywords": [
    "css",
    "json",
    "json",
    "to",
    "css",
    "json",
    "to",
    "css",
    "converter"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aramk/CSSJSON.git"
  },
  "dependencies": {
    "through2": "^4.0.2",
    "yargs": "^17.7.2"
  }
